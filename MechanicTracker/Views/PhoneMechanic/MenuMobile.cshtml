

<div class="container">
    <div class="row">
        <div class="col-xs-12">
            <h1 class="text-center"><b>0:00</b></h1>
        </div>
    </div>
    <div class="row" style="margin-top:20px;">
        <div class="col-xs-6">
            <button  class="btn btn-start" style="width:100%;height:70px;"><i class="fa fa-fw fa-play"></i><br /><b>Start Job</b></button>
        </div>
        <div class="col-xs-6">
            <button  class="btn btn-stop" style="width:100%;height:70px;"><i class="fa fa-fw fa-stop"></i><br /><b>Stop Job</b></button><br />
        </div>
    </div>
    <div class="row" style="margin-top:20px;">
        <div class="col-xs-6">
            <button  class="btn btn-pause" style="width:100%;height:70px;"><i class="fa fa-fw fa-pause"></i><br /><b>Pause Job</b></button>
        </div>
        <div class="col-xs-6">
            <button  class="btn btn-complete" style="width:100%;height:70px;"><i class="fa fa-fw fa-forward"></i><br /><b>Complete</b></button><br />
        </div>
    </div>

    <div class="row" style="margin-top:20px;">
        <div>
             <div class="form-group">
                <label class="col-sm-5 control-label forms">Name:</label>
                <div class="col-sm-3 input-container">
                    <span data-bind="text: Name"> </span>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-5 control-label forms">Description:</label>
                <div class="col-sm-3 input-container">
                    <span data-bind="text: Description"> </span>
                </div>
            </div>
        </div>
    </div>
</div> <!--/Container-->


@Scripts.Render("~/bundles/Knockout")